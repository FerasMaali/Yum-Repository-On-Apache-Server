#!/usr/bin/env bash

result=$(docker run --rm repo_client:latest 'rpm -qa' | grep 'httpd-2.4.6-93.el7.centos.x86_64')

if [ $result ]
then
	echo "********************************************************"
	echo "+++++++++++++++++Success"
	echo "********************************************************"
	exit 0
else
	echo "********************************************************"
	echo "--------------------Fail"
	echo "********************************************************"
	exit 1
fi

